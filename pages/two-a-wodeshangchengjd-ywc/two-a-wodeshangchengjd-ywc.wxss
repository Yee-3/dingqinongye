/* 物流弹框 */
.wl_1 {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.4);
}

.wl_2 {
  position: fixed;
  left: 90rpx;
  right: 90rpx;
  top: 50%;
  transform: translateY(-50%);
  background-color: #fff;
  border-radius: 50rpx;
  text-align: center;
  padding: 40rpx;
  box-sizing: border-box;
}

