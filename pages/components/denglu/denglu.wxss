/* pages/components/denglu/denglu.wxss */
/* 登录弹框 */
.dl_01 {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}

.dl_02 {
  position: fixed;
  height: 285rpx;
  left: 90rpx;
  right: 90rpx;
  top: 50%;
  transform: translateY(-50%);
  border-radius: 45rpx;
  background: #fff;
  text-align: center;
}

.dl_02 button image {
  width: 360rpx;
 height: 70rpx;
}

.dl_02 button {
  max-width: 360rpx;
  max-height: 70rpx;
  padding: 0;
  border-radius: 45rpx;

}
.dl_03{
  size: 34rpx;
  margin-top: 60rpx;
  margin-bottom: 50rpx;
  font-weight: 600;
}